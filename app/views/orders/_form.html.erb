<h1>place order</h1>

<%= form_with model: [@product, @order ] do |form| %>

   <div>
    <%= form.label :address %><br>
    <%= form.text_field :address %>
  </div>


  <div>
    <%= form.label :phone_no %><br>
    <%= form.text_field :phone_no %>
  </div>
  

  <div>

    <%= form.submit %>
  </div>
<% end %>