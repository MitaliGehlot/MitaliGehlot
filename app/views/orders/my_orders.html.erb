<button><%= link_to "download", my_orders_path %></button>
<table class="table table-striped table-class" id= "table-id" style="border-collapse: separate;" style="border-spacing: 0px 15px;" >
 <thead>  
  <tr>
    <th>Product name </th>
    <th>product model </th>  
    <th>product amount </th>
    <th>payment type </th>  
    <th>product price </th>
    <th> Date </th>
  </tr>  
  </thead>

  <tbody>
  <% @my_orders.each do |order| %>
  <tr>
    <td style="width:150px"><%= order.product.name %></td>
    <td style="width:150px"><%= order.product.model %></td>
    <td style="width:150px"><%= order.amount %></td>
    <td style="width:150px"><%= order.payment_type %></td>
    <td style="width:150px"> Rs.<%= order.product.price %></td>
    <td style="width:150px"> <%= order.product.created_at %></td>
  </tr>

<% end %>   

</tbody>
</table> 
