
<%= form_with model: [@category] do |form| %>
 <div>
    <%= form.label :CategoryName %><br>
    <%= form.text_field :category_name %>
        <% @category.errors.full_messages_for(:category_name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>